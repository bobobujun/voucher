.code_bg{
  position: fixed;
  top:0;
  left: 0;
  right:0;
  bottom:0;
  background-color: rgba(0,0,0,.5);
  z-index: 999;
}
.icon-login-bg{
  background-image: url('data:image/png;base64,');
  background-repeat: no-repeat;
}
.code-con{
  position: absolute;
  top:30%;
  /* width: 320px; */
  left: 50%;
  margin-left: -320rpx;
  background-color: #fff;
  z-index: 1000;
  -moz-user-select: none;
  -webkit-user-select: none;
}
.code-img{
  margin: 10rpx 10rpx;
  padding: 10rpx 10rpx;
  background-color: #f5f6f7;
}
.code-img image{
  display: block;
}
.code-front-img{
  width: 60px;
  height: 60px;
}
.icon-w-25{
  display: inline-block;
  width: 50rpx;
  height: 50rpx;
  text-indent: -9999px;
}
.icon-push{
  cursor: pointer;
  background-position: -300rpx -200rpx;
}
.code-push{
  height: 50rpx;
}
.code-btn{
  position: relative;
  height: 60rpx;
  text-align: center;
  color: #999;
  margin: 20rpx 20rpx;
  box-sizing: border-box;
  background-color: #f5f6f7;
  border-radius: 30rpx;
  border: 1px solid #e1e1e1;
}
.code-btn-m{
  position: absolute;
  width: 80rpx;
  height: 80rpx;
  border-radius: 50%;
  background-color: #f5f6f7;
  border: 1px solid #e1e1e1;
  z-index: 5;
  top:-16rpx;
  /* left: 20rpx; */
  box-shadow: 0 0 3px #ccc;
  cursor: pointer;
  background-position: -126rpx 20rpx;
}
.code-btn-img{
  background-image:url('data:image/png;base64,');
  background-repeat: no-repeat;
} 
.code-btn-img.active{
  background-position: -268rpx 20rpx;
}
.code-span{
  line-height: 56rpx;
}
.code-btn-img.error{
  background-position: 16rpx 20rpx;
}
.code-img-con{
  position: relative;
}
.code-mask{
  position: absolute;
  top:0;
  left: 0;
  z-index: 10;
}
.code-tip{
  line-height: 60rpx;
  padding-left: 20rpx;
  font-size: 24rpx;
  color: #999;
}
.code-tip-red{
  color: red;
}
.code-tip-green{
  color: green;
}
.code-back-img{
  width: 300px;
  height: 150px;
}
.shandowing{
  box-shadow: 0 0 8px #666;
}
.boxshadowend{
  box-shadow: 0 0 3px #ccc;
}
.transition{
  transition: left 0.5s;
}